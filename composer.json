{
    "name": "hyperframework/hyperframework",
    "type": "library",
    "description": "A simple and professional PHP framework.",
    "keywords": ["framework"],
    "homepage": "http://hyperframework.com",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperframework\\": "lib"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Hyperframework\\Cli\\Test\\": "test/cli/lib",
            "Hyperframework\\Db\\Test\\": "test/db/lib",
            "Hyperframework\\Logging\\Test\\": "test/logging/lib",
            "Hyperframework\\Web\\Test\\": "test/web/lib",
            "Hyperframework\\Common\\Test\\": "test/common/lib",
            "Hyperframework\\Test\\": "test/lib"
        }
    }
}
